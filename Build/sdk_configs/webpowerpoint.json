                    "<%= pkg.menu_path %>/PowerPoint/": [
                        "../PowerPoint/sdk-all.js.map",
                        "../PowerPoint/Build/Log/property.map",
                        "../PowerPoint/Build/Log/variable.map",
                        "../PowerPoint/Build/Log/apiExport.js"
                    ]
                },
