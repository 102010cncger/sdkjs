            "../PowerPoint/api.js",
            "../PowerPoint/apiCommon.js"
        ]
    },
    "extract_api": {
        "src": "../PowerPoint/api-all.js",
        "dst": "../PowerPoint/Build/Log/apiExport.js"
    },
    "compile": {
        "sdk": {
