                "../PowerPoint/Editor/Drawing/GroupShape.js",
                "../PowerPoint/Editor/Drawing/Joined.js",
                "../Word/Drawing/Graphics.js",
                "../Word/Drawing/Overlay.js",
                "../Word/Drawing/ShapeDrawer.js",
				"../PowerPoint/Drawing/Transitions.js",
